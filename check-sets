#!/bin/sh

rm -f sets/*~
cat sets/* |sort |uniq >/tmp/cs-sets
find / | egrep -v '^(/$|/dev/|/proc/|/tmp/|/root/|/src/linux-2.6)' | sort >/tmp/cs-all

comm -13 /tmp/cs-all /tmp/cs-sets >/tmp/cs-missing
comm -23 /tmp/cs-all /tmp/cs-sets >/tmp/cs-superfluous

echo $(wc -l /tmp/cs-missing |cut -d" " -f 1) files missing:
cat /tmp/cs-missing
echo

echo $(wc -l /tmp/cs-superfluous |cut -d" " -f 1) files missing:
cat /tmp/cs-superfluous
echo
