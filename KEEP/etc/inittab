# Note that busybox uses a non-standard inittab.

::sysinit:/etc/rc.boot
::respawn:/bin/runsvdir-start
::shutdown:/etc/rc.shutdown

tty1::respawn:/bin/getty -L 38400 tty1 linux
tty2::respawn:/bin/getty -L 38400 tty2 linux
tty3::respawn:/bin/getty -L 38400 tty3 linux
tty4::respawn:/bin/getty -L 38400 tty4 linux
